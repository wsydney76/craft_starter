{
  "name": "wsydney76/craft_starter",
  "version": "2.0.0",
  "description": "Simple multilingual project template for Craft CMS",
  "license": "Unlicense",
  "require": {
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/cms": "3.7.7",
    "craftcms/contact-form": "2.2.7",
    "doublesecretagency/craft-cpcss": "2.3.0",
    "spicyweb/craft-embedded-assets": "2.7.0",
    "wsydney76/work": "^1.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "config\\": "config/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('config/Env.php') || copy('config/Env.php.example', 'config/Env.php');\""
    ],
    "pre-update-cmd": [
      "@php craft off"
    ],
    "post-update-cmd": [
      "@php craft clear-caches/all",
      "@php craft migrate/all --interactive=0",
      "@php craft on"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/wsydney76/work"
    },
    {
      "type": "path",
      "url": "e:/craft/plugins/plugins/*"
    }
  ]
}
